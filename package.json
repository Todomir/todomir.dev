{
  "name": "abn.ooo",
  "private": true,
  "description": "My really Qwik personal portfolio",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,css,md,mdx,json}\"",
    "i18n-extract": "qwik-speak-extract --supportedLangs=en,pt-BR --assetsPath=i18n",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "motion": "^12.9.2",
    "og-img": "^0.2.1",
    "split-type": "^0.3.4"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.13.0",
    "@builder.io/qwik-city": "^1.13.0",
    "@builder.io/qwik-react": "0.5.0",
    "@content-collections/core": "^0.8.2",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/vite": "^0.2.4",
    "@eslint/js": "^9.25.1",
    "@evilmartians/harmony": "^1.4.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.4",
    "@todomir/eslint-config": "^10.1.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.2",
    "@unpic/qwik": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.1",
    "eslint-plugin-qwik": "^1.13.0",
    "glob": "^11.0.2",
    "globals": "^16.0.0",
    "gray-matter": "^4.0.3",
    "postcss": "^8.5.3",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "qwik-speak": "^0.23.0",
    "rehype-pretty-code": "^0.14.1",
    "tailwindcss": "4.1.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.0",
    "undici": "^7.8.0",
    "unist-util-visit": "^5.0.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.13.2",
    "zod": "^3.24.3",
    "zod-to-ts": "^1.2.0"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts"
}
