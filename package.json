{
  "name": "todomir.dev",
  "description": "My really Qwik personal portfolio",
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "format": "prettier --write \"**/*.{js,ts,tsx,css}\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "i18n-extract": "qwik-speak-extract --supportedLangs=en,pt-BR --assetsPath=i18n",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.3.2",
    "@builder.io/qwik-city": "^1.3.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.10",
    "@todomir/prettier-config": "^4.0.0",
    "@types/eslint": "^8.56.1",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-qwik": "^1.3.2",
    "fontaine": "^0.4.1",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "qwik-speak": "^0.19.0",
    "tailwindcss": "3.4.0",
    "typescript": "^5.3.3",
    "undici": "^6.2.1",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "wrangler": "^3.22.3"
  },
  "dependencies": {
    "@evilmartians/harmony": "^1.2.0",
    "@unpic/qwik": "^0.0.38",
    "motion": "^10.16.4",
    "rehype-pretty-code": "^0.12.3",
    "shikiji": "^0.9.17",
    "valibot": "^0.25.0"
  }
}
