{
  "name": "todomir.dev",
  "description": "My really Qwik personal portfolio",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "check": "biome check --apply-unsafe --vcs-use-ignore-file=true --files-ignore-unknown=true .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.0",
    "@builder.io/qwik": "^1.2.19",
    "@builder.io/qwik-city": "^1.2.19",
    "@types/eslint": "^8.44.4",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.51.0",
    "eslint-plugin-qwik": "^1.2.19",
    "postcss": "^8.4.31",
    "tailwindcss": "3.3.3",
    "typescript": "^5.2.2",
    "undici": "^5.26.0",
    "vite": "^4.4.11",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "@evilmartians/harmony": "^1.2.0"
  }
}
